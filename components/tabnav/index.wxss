/* components/tabnav/index.wxss */
@import '/app.wxss';


.tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 110rpx;
  border-top: 0.5px solid #d5d5d5;
  display: flex;
  font-size: 0;
  background: #fff;
}
.tabbar-item {
  flex: 1;
  text-align: center;
  overflow: hidden;
  box-sizing: border-box;
  padding: 8rpx 10rpx 0;
  color: #333333;
}

.tabbar-item-icon {
  height: 59rpx;
}

.tabbar-item-icon image {
width: 52rpx;
height: 52rpx;
margin-top: .1rem;

}

.tabbar-item-text {
  font-size: 25rpx;
  color: #999;
}
.active {
  color: #ff443b;
}