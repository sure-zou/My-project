/* pages/user_gocash/index.wxss */
.page{
  background-color: #f5f5f5;
}
.cash{
  width: 92%;
  margin:  0 auto;
  border-radius: 10rpx;
}
.cash .name{
  font-size:33rpx;
  color: #333333;
  line-height: 1.7;
  font-weight: 700;
}
.cash .name  .text2{
  font-size: 30rpx;
  color: #898989;
  font-weight: normal
}
.cash_list{
padding: 5% 6%  4%  6%;
background: #ffffff;
margin-top: .8rem;
border-radius: 10rpx;
box-shadow: 0 0 10rpx  #dfdfdf;
}
.cash_list .btn{
 color: #ffffff;
 padding: .36rem  1rem;
border-radius: 1rem;
}
 .news{
height: 77rpx;
border-top: 1rpx solid #f4f4f4;
padding: 0 30rpx;
box-shadow: 0 10rpx 30rpx #f5f5f5;
background: #ffffff;
margin: 0 auto;
margin-top: .38rem;
position: fixed;
bottom: 0;
width: 100%;
}
 .news .pictrue{width:40rpx;height:30rpx;border-right:1rpx solid #ddd;padding-right:23rpx;box-sizing: content-box;}
.news .pictrue image{width:100%;height:100%;}
.news .swiperTxt{width:87%;height:100%;line-height:77rpx;overflow:hidden;}
 .news .swiperTxt .text{width:100%;}
.news .swiperTxt .text .label{font-size:20rpx;color:#ff4c48;width:64rpx;height:30rpx;border-radius:40rpx;text-align:center;line-height:28rpx;border:2rpx solid #ff4947;}
 .news .swiperTxt .text .newsTitle{width:100%;font-size:24rpx;color:#666;}
 .news .swiperTxt .iconfont{font-size:28rpx;color:#888;}
 .news .swiperTxt swiper{height:100%;}
